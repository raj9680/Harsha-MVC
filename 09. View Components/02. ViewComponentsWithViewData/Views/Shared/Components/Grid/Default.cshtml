@using ViewComponentsWithViewData.Models;
<div class="box">
    <h3>Title: @ViewBag.Grid.GridTitle</h3></br>
    <table class="table w-100">
        <thead>
            <tr>
                <th>Sr. No</th>
                <th>Name</th>
            </tr>
        </thead>
        <tbody>
            @foreach (PersonModel person in ViewBag.Grid.Persons)
            {
                <tr>
                    <td>@person.PersonName</td>
                    <td>@person.JobTitle</td>
                </tr>
            }
        </tbody>
    </table>
</div>