    <ImplicitUsings>enable</ImplicitUsings>
    <RootNamespace>Contacts_Manager</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <_ContentIncludedByDefault Remove="countries.json" />
  </ItemGroup>

  <ItemGroup>
